{
  "name": "redoc",
  "version": "0.0.1",
  "scripts": {
    "start": "node_modules/.bin/bsb -make-world -backend native",
    "watch": "node_modules/.bin/bsb -make-world -backend native -w",
    "postinstall": "node_modules/.bin/bsb -make-world -backend native"
  },
  "bin": "./redoc.sh",
  "keywords": [
    "BuckleScript",
    "documentation",
    "generator"
  ],
  "repository": {
    "url": "jaredly/redoc",
    "type": "git"
  },
  "description": "A clean & easy documentation generator for reason/bucklescript/ocaml",
  "author": "Jared Forsyth",
  "license": "ISC",
  "dependencies": {
    "bs-platform": "bsansouci/bsb-native#fast",
    "pack.re": "jaredly/pack.re",
    "pack.re-bin": "*",
    "minimist.re": "0.0.5",
    "rex-json": "1.3.0"
  }
}
